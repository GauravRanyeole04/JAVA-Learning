class Population1
{
public static void main(String[]args)
	{
		Scanner s=new Scanner(System.in);

		long g=((24*60*60)*365);
		
		System.out.print("How many sec have 1 birth : ");
		int num1=s.nextInt();
		long birth=g/num1;
		//System.out.println("birth per year : "+birth);
		
		System.out.print("How many sec have 1 death : ");
		int num2=s.nextInt();
		long death=g/num2;
		//System.out.println("death per year : "+death);
		
		System.out.print("How many sec have 1 immigrant : ");
		int num3=s.nextInt();
		long immigrant=g/num3;
		//System.out.println("immigrant per year : "+immigrant);
		
		System.out.print("current population : ");
		int num4=s.nextInt();

		System.out.print("Calculation for How many year : ");
		int num5=s.nextInt();

		
		long Total_Population=((birth-death+immigrant)*num5)+num4;
		System.out.print("Total population : "+Total_Population);

		}
}

